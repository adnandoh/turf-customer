{
  "name": "turfbook-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "clean": "rmdir /s /q .next out 2>nul || echo Clean completed",
    "clear-cache": "node scripts/clear-cache.js",
    "validate-sitemap": "node scripts/validate-sitemap.js",
    "optimize-images": "node scripts/optimize-images.js",
    "performance-audit": "node scripts/performance-audit.js",
    "code-audit": "node scripts/code-audit.js",
    "performance-summary": "node scripts/performance-summary.js",
    "cleanup-images": "node scripts/cleanup-old-images.js",
    "deploy": "npm run optimize-images && npm run build && npm run clear-cache"
  },
  "dependencies": {
    "@next/third-parties": "^16.1.1",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "postcss": "^8",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "xmldom": "^0.6.0"
  }
}
